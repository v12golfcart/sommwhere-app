{
  "expo": {
    "name": "Sommwhere",
    "slug": "sommwhere",
    "scheme": "sommwhere",
    "displayName": "Sommwhere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sommwhere.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.sommwhere.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "v12golfcart",
    "extra": {
      "eas": {
        "projectId": "ec235518-402d-4297-8376-16dd5beece5a"
      }
    },
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to capture wine photos",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone (not used)",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to select wine images"
        }
      ]
    ]
  }
}
